	email = 88557639+lishaduck@users.noreply.github.com
	name = Eli

[github]
	user = lishaduck

[core]
	excludesfile = ~/.gitignore_global
	editor = code --wait
	ignorecase = false
	filemode = true
	repositoryformatversion = 1
	logAllRefUpdates = true
	autocrlf = input

[init]
	defaultbranch = main

[pull]
	rebase = true

[push]
	default = upstream
	followTags = true

[fetch]
	prune = true

[grep]
	lineNumber = true

[alias]
	amend = commit --amend --reuse-message=HEAD
